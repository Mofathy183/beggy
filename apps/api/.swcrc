{
	"$schema": "https://swc.rs/schema.json",
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"decorators": true,
			"dynamicImport": true
		},
		"target": "es2022",
		"loose": false,
		"externalHelpers": false,
		"keepClassNames": true,
		"baseUrl": "./src",
		"paths": {
			"@/*": ["*"]
		}
	},
	"module": {
		"type": "es6",
		"strict": true,
		"strictMode": true,
		"lazy": false,
		"noInterop": false
	},
	"minify": false,
	"sourceMaps": true,
	"inlineSourcesContent": true
}

